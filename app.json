{
  "expo": {
    "name": "walker",
    "slug": "walker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lsandini.walker",
      "buildNumber": "7",
      "infoPlist": {
        "NSHealthShareUsageDescription": "This app uses HealthKit to access your step count for tracking and analysis.",
        "NSHealthUpdateUsageDescription": "This app uses HealthKit to track your steps for fitness monitoring.",
        "UIBackgroundModes": [
          "fetch",
          "remote-notification",
          "processing"
        ],
        "BGTaskSchedulerPermittedIdentifiers": [
          "com.transistorsoft.fetch"
        ]
      }
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lsandini.walker",
      "permissions": [
        "android.permission.health.READ_STEPS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e985fdca-83d6-499a-8ddb-9140c993f541"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "14.0"
          },
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 26
          }
        }
      ],
      "react-native-health",
      "expo-health-connect",
      "./withEntitlementsPlist.js",
      "react-native-background-fetch"
    ]
  }
}
